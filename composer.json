{
    "name": "arturchel174/yii2-cart",
    "description": "yii2-cart it's a simple cart module to collect application models, that implement cart interface + widgets",
    "keywords": ["yii2", "yii 2", "application", "cart", "shop", "module", "dvizh"],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.4.0",
        "yiisoft/yii2": "~2.0.45",
        "yiisoft/yii2-bootstrap5": "@dev",
        "yiisoft/yii2-bootstrap4": "@dev"
    }
    ,
    "require-dev": {
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "process-timeout": 1800
    },
    "autoload": {
        "psr-4": {
            "dvizh\\cart\\": "src\\"
        }
    },
    "extra": {
        "bootstrap": "dvizh\\cart\\Bootstrap"
    }
}
